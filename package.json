{
  "name": "Elmctron",
  "version": "0.1.0",
  "author": "Nir Galon <nirgn975@gmail.com (http://nirgn.com)",
  "summary": "TodoMVC Elm and Electron.",
  "main": "main-electron.js",
  "repository": "https://github.com/nirgn975/Elmctron.git",
  "devDependencies": {
    "browser-sync": "^2.13.0",
    "electron-builder": "^5.22.1",
    "electron-connect": "^0.4.2",
    "electron-prebuilt": "^1.3.1",
    "electron-reload": "^1.0.1",
    "elm": "^0.17.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-cssmin": "^0.1.7",
    "gulp-elm": "^0.5.0",
    "gulp-sass": "^2.3.2",
    "typescript": "^1.8.10"
  },
  "dependencies": {
    "electron-reload": "^1.0.1"
  },
  "scripts": {
    "gulp": "gulp",
    "pack": "build --dir",
    "dist": "build"
  },
  "description": "A todo app.",
  "homepage": "http://yourAppHomePage.com",
  "build": {
    "appId": "your.id",
    "app-category-type": "public.app-category.productivity",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "win": {
      "iconUrl": "https://github.com/nirgn975/Elmctron"
    },
    "linux": {
      "target": ["deb", "AppImage"]
    }
  }
}
